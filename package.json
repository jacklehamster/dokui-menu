{
  "name": "dokui-menu",
  "module": "src/index.ts",
  "description": "UI menu for Dobuki Game Engine.",
  "version": "1.0.45",
  "keywords": [
    "bun",
    "bunjs",
    "typescript",
    "template",
    "npm",
    "np",
    "publish",
    "git"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:jacklehamster/dokui-menu.git"
  },
  "homepage": "https://jacklehamster.github.io/dokui-menu/",
  "author": {
    "name": "Jack Le Hamster",
    "email": "jacklehamster@gmail.com"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "test": "bun test",
    "build": "bun i && rm -rf *.tsbuildinfo && rm -rf dist/*.* && bun run bundler/bundler.ts && bun run test && bun run build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly --project tsconfig.types.json",
    "build:example": "cd example && bun i && bun run build && cd ..",
    "example": "cd example && bun i && bun start && cd ..",
    "fix-package-name": "npm explore package-name-fixer -- bun start \"$(pwd)\"",
    "auto-publish": "npm explore @dobuki/auto-publish -- bun run auto-publish \"$(pwd)\""
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dobuki/auto-publish": "^1.0.7",
    "@types/bun": "^1.1.2",
    "@types/jest": "^29.5.6",
    "@types/mocha": "^10.0.3",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^9.0.8",
    "bun": "^1.1.8",
    "bun-types": "^1.1.8",
    "jest": "^29.7.0",
    "package-name-fixer": "^1.0.9",
    "prettier": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "typescript": "^5.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "@dobuki/react-picture": "^1.0.2",
    "@dobuki/react-popup": "^1.0.7",
    "@etsoo/shared": "^1.2.36",
    "abstract-list": "^1.0.15",
    "hangul-js": "^0.2.6",
    "react-contenteditable": "^3.3.7",
    "styled-components": "^6.1.11",
    "unique-layout": "^1.0.3",
    "uuid": "^9.0.1"
  }
}
